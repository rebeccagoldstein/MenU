    "url": "https://github.com/ryanhartlee/DrinkMaker/issues"
  },
  "homepage": "https://github.com/ryanhartlee/DrinkMaker#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.11"
  },
  "dependencies": {
    "@restart/context": "2.1.2",
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.4",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "express": "^4.16.4",
    "express-router-methods": "^1.0.3",
    "express-session": "^1.15.6",
    "if-env": "^1.0.4",
    "mongo": "^0.1.0",
    "mongoose": "^5.4.22",
